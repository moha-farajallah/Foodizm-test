<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="gmo_card_head_components" template="Veriteworks_Gmo::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Veriteworks\Gmo\Block\Card\Lists" name="gmo.cards.lists" cacheable="false" template="card/lists.phtml" />
        </referenceContainer>
    </body>
</page>
